//ex 12     (abbc)

#include <iostream>
#include <cstring>

using namespace std;

int main()
{
    char s[100];
    int n;
    cin.get(s, 100);
    cout<<s[1]<<endl;;
    n=strlen(s);
    for(int i=0; i<n; i++)
    {
        for(int j=0; j<n; j++)
        {
            if(i!=j)
                cout<<s[j];
        }
        cout<<endl;
    }
    return 0;
}



