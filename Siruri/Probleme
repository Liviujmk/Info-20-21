#include <iostream>
#include <cstring>

using namespace std;

void invers(char k[100]) {
    char temp;
    int i=0, d=0;
    while(k[d]!='\0'){
        d++;
    }
    d = d-1;
    while(i<d) {
        temp = k[i];
        k[i] = k[d];
        k[d] = temp;
        d--;
        i++;
    }
    cout<<k;
}

